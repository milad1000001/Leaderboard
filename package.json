{
  "name": "leaderboard",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "storybook": "start-storybook",
    "generate:icons": "vsvg -s ./src/assets/raw-svg -t ./src/assets/compiled-svg --es6",
    "generate:symlinks": "link-module-alias",
    "prepare": "yarn generate:icons && yarn generate:symlinks",
    "preserve": "yarn prepare",
    "postinstall": "yarn prepare",
    "postuninstall": "yarn prepare"
  },
  "dependencies": {
    "@babel/core": "^7.10.5",
    "@storybook/vue": "^5.3.19",
    "babel-loader": "^8.1.0",
    "babel-preset-vue": "^2.0.2",
    "core-js": "^3.6.5",
    "destyle.css": "^1.0.11",
    "link-module-alias": "^1.2.0",
    "precss": "^4.0.0",
    "tailwindcss": "^1.4.6",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.3",
    "vue-persian-filters": "^0.2.4",
    "vue-router": "^3.2.0",
    "vue-svgicon": "^3.2.6",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.4.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "postcss": "^7.0.27",
    "postcss-aspect-ratio": "^1.0.2",
    "postcss-import": "^12.0.1",
    "postcss-shape": "^0.0.1",
    "postcss-short": "^5.0.0",
    "sass-loader": "^9.0.2",
    "stylelint": "^13.3.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "vue-cli-plugin-storybook": "^1.3.0",
    "vue-template-compiler": "^2.6.11"
  },
  "_moduleAliases": {
    "~root": ".",
    "~src": "./src",
    "~assets": "./src/assets",
    "~images": "./src/assets/images",
    "~styles": "./src/assets/styles",
    "~atoms": "./src/components/atoms",
    "~molecules": "./src/components/molecules",
    "~organisms": "./src/components/organisms",
    "~layouts": "./src/components/layouts",
    "~pages": "./src/components/pages",
    "~icons": "./src/assets/compiled-svg",
    "~services": "./src/services",
    "~api": "./src/api",
    "~api$": "./src/api/index.js",
    "~router$": "./src/router/index.js"
  }
}
